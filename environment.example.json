{
  "default": {
    "config": {
      "cache": false,
      "minify": false,
      "concat": false,
      "tag": {
        "stylesheet": "<link rel=\"stylesheet\" type=\"text/css\" href=\"#path\" media=\"screen\">",
        "javascript": "<script src=\"#path\"></script>"
      },
      "path": {
        "stylesheet": "styles/common.css",
        "javascript": "script/common.js"
      }
    },
    "stylesheet": [
      "styles/custom.less",
      "styles/libs/normalize.css",
      "styles/libs/bootstrap/bootstrap.min.css"
    ],
    "javascript": [
      "scripts/libs/modernizr.min.js",
      "scripts/libs/jquery.min.js",
      "scripts/libs/bootstrap.min.js",
      "scripts/custom.js"
    ],
    "content": "content/"
  },
  "development": {
    "config": {
      "minify": true
    }
  },
  "production": {
    "config": {
      "cache": true,
      "minify": true,
      "concat": true
    }
  }
}